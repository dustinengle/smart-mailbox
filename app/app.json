{
  "expo": {
    "name": "Its Here",
    "slug": "its-here",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "scheme": "itshere",
    "ios": {
      "bundleIdentifier": "io.pagarba.ItsHere",
      "supportsTablet": true
    },
    "android": {
      "package": "io.pagarba.ItsHere",
      "permissions": [
        "CAMERA",
        "READ_CONTACTS",
        "READ_PHONE_STATE"
      ]
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
